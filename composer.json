{
    "name": "karpack/punch",
    "description": "Generic permission and role handler package",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/karpack/punch/issues",
        "source": "https://github.com/karpack/punch"
    },
    "keywords": ["laravel", "punch", "permission", "roles"],
    "authors": [
        {
            "name": "Kalesh Kaladharan",
            "email": "kalesh.13@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Karpack\\Punch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karpack\\Punch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Karpack\\Punch\\PunchServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
